@model List<RentACar.Models.Car>

<h1>najpopularniji automobili 90-ih</h1>

<table class="table table-striped">
	<thead>
		<tr>
			<th>Make</th>
			<th>Model</th>
			<th>Year</th>
			<th>Price per day</th>
			<th>Availability</th>
			<th>Seats</th>
		</tr>
	</thead>
	<tbody>
		@foreach (Car singleCar in Model)
		{
			<tr>
				<td>@singleCar.Make</td>
				<td>@singleCar.Model</td>
				<td>@singleCar.Year</td>
				<td>@singleCar.PricePerDay</td>
				<td>@singleCar.isAvailable</td>
				<td>@singleCar.Seats</td>
			</tr>
		}
	</tbody>
</table>